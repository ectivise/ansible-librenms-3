com2sec readonly  default         {{ librenms_snmpd_community_string }}

group {{customer_site}} v2c        readonly
view all    included  .1                               80
access {{customer_site}} ""      any       noauth    exact  all    none   none

syslocation {{ librenms_snmpd_syslocation }}
syscontact {{ librenms_snmpd_syscontact }}

